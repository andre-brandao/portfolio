{
	"useTabs": false,
	"tabWidth": 3,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	
	
	"bracketSameLine": false,
	"arrowParens": "always",
	"htmlFormat":{
		"wrapAttributes": "force-expand-multiline"
	}
}
